"use strict";

const apeTmpl = require('ape-tmpl')
const pkg = require('./package.json')

module.exports = apeTmpl.licenseBud({
  type: pkg.license,
  holder: pkg.repository.split(/\//).shift(),
  year: 2015
})

if (!module.parent) {
  require('coz').render(__filename)
}
